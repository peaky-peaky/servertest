user create --domain default --project service --password servicepassword glance
role add --project service --user glance admin
service create --name glance --description "OpenStack Image service" image
endpoint create --region RegionOne image public http://7.1.1.20:9292
endpoint create --region RegionOne image internal http://7.1.1.20:9292
endpoint create --region RegionOne image admin http://7.1.1.20:9292
